 "html": "    <style>\r\n        table {\r\n            border-collapse: collapse;\r\n            width: 100%;\r\n        }\r\n\r\n        th,\r\n        td {\r\n            text-align: center;\r\n            border: 1px solid;\r\n             Add border to all cells \r\n        }\r\n\r\n        th {\r\n            background-color: #f2f2f2;\r\n        }\r\n\r\n         Remove border for specific cells \r\n        .no-border {\r\n            border: none;\r\n        }\r\n\r\n        * {\r\n            font-size: 11px;\r\n        }\r\n    </style>\r\n\r\n    <div class=\"table-container\">\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th style='font-size:14px;color: black;'>Sr No: {{ doc.name }}</th>\r\n                   <!-- <th style=\"font-size:14px;color: black;\">{{ doc.name }}</th> -->\r\n\r\n                    <th style='font-size:14px;color: black;'>Date: {{ doc.get_formatted(\"date\", doc) }}</th>\r\n                   <!-- <th style=\"font-size:14px;color: black;\">{{ doc.get_formatted(\"date\", doc) }}</th> -->\r\n\r\n                    <th style='font-size:14px;color: black;'>Due Date: {{ doc.get_formatted(\"due_date\", doc) }}</th>\r\n                    <!--<th style=\"font-size:14px;color: black;\">{{ doc.get_formatted(\"due_date\", doc) }}</th> -->\r\n\t\t\t\t\t\r\n\t\t\t\t\t<th style='font-size:14px;color: black;'>Priority: {{ doc.priority }}</th>\r\n\t\t\t\t\t\r\n                </tr>\r\n            </thead>\r\n        </table>\r\n        \r\n    <div style=\"border: 1px;\">\r\n        <table style=\"width:100%; border: 1px solid;\">\r\n           <thead >\r\n             <tr style='width:25%;border-left: 1px solid;'>\r\n                <th style='width:15%; border-right: 1px solid; text-align: left; font-size:18px;color: black;'>Customer:  {{ doc.customer }}</th>\r\n             </tr>\r\n\r\n             <tr style='width:25%;border-left: 1px solid;'>\r\n                <th style='width:15%; border-right: 1px solid; text-align: left; font-size:18px;color: black;'>Reference:  {{ doc.reference_no or \"\" }}</th>\r\n             </tr>\r\n\t\t\t \r\n             <tr style='width:100%;border-left: 1px solid;'>\r\n                <th style= 'width:100%; border-right: 1px solid; text-align: left; font-size:16px;display:flex;color: black;'><p style='width:100px;'>Job Description:</p>  {{ doc.job_description }}</th>\r\n             </tr>\r\n           </thead>\r\n       </table>\r\n    </div>\r\n    \r\n    <div style=\"border: 1px;\">\r\n        <table style=\"width:100%; border: 1px solid;\">\r\n           <thead >\r\n              <tr style='width:25%;border-left: 1px solid;'>\r\n                <!-- <th style='width:15%; border-right: 1px solid; text-align: left;color: black;'>Date: {{ doc.get_formatted(\"date\", doc) }}</th> -->\r\n                <!-- <th style='width:18%; border-right: 1px solid; text-align: left;color: black;'>Reference No: {{ doc.reference_no or \"\" }} </th> -->\r\n                <!--<th style='width:15%; border-right: 1px solid;text-align: left;color: black;'>Priority : {{ doc.priority }} </th> -->\r\n                <!-- <th style='width:27%; border-right: 1px solid;text-align: left;color: black;'>Status :  {{ doc.workflow_state }}</th> -->\r\n                <!-- <th style='width:20%; text-align: left;color: black;'>Due Date : {{ doc.get_formatted(\"due_date\", doc)}} </th> -->\r\n             </tr>\r\n           </thead>\r\n       </table>\r\n    </div>\r\n    \r\n        \r\n        {% for row in doc.process_multiple_table %}\r\n        <div>\r\n            <table>\r\n                <thead>\r\n                    <tr style=\"height: 30px;\">\r\n                        <th style=\"width: 40%; color: black;\"><b style='font-size: 15px;'>{{ row.process_title }}</b></th>\r\n                        <th style=\"width: 60%;\">\r\n                            <svg class=\"barcode\"\r\n                            jsbarcode-margin=\"0\"\r\n                            jsbarcode-flat=\"true\"\r\n                            jsbarcode-height=\"40\" \r\n                            jsbarcode-width=\"2\"\r\n                            jsbarcode-displayValue=\"false\" \r\n                            jsbarcode-value=\"{{ row.barcode }}\" /> \r\n                        </th>\r\n                    </tr>\r\n                </thead>\r\n            </table>\r\n        </div>\r\n        <div>\r\n            <table>\r\n                <thead>\r\n                    <tr>\r\n                        <td style='text-align: left;'>First off sign :_______________</td>\r\n                        <td style='text-align: left border-right : none;'>Production Qty :____________</td>\r\n                        <td style='text-align: left;'>QA Inspect :______________</td>\r\n                    </tr>\r\n                </thead>\r\n            </table>\r\n        </div>\r\n        {% endfor %}\r\n        <svg id=\"test\"></svg>\r\n    </div>\r\n\r\n    <script src=\"assets/frappe/node_modules/jsbarcode/dist/JsBarcode.all.min.js\"></script>\r\n    <script>\r\n        JsBarcode(\".barcode\").init();\r\n    </script>",
